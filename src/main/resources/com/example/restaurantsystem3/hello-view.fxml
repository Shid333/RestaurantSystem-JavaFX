<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="356.0" prefWidth="272.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.restaurantsystem3.managerLogin">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label fx:id="lblUsername" text="Username" />
   <TextField fx:id="txtUsername" promptText="Username" />
   <Label fx:id="lblPassword" text="Password" />
   <TextField fx:id="txtPassword" promptText="Password" />
   <Button fx:id="btnEnter" mnemonicParsing="false" onAction="#pressEnter" text="Sign in!" />
   <Button fx:id="btnBack" mnemonicParsing="false" onAction="#btnBack" text="Back" />

    <Label fx:id="welcomeText" />
   <Label fx:id="lblShow" prefHeight="24.0" prefWidth="251.0" text="" />
   <AnchorPane prefHeight="200.0" prefWidth="200.0" />
</VBox>
